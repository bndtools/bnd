	org.junit.vintage.engine,\
	org.opentest4j,\
	test;version=snapshot,\
	test-fragment;version=snapshot,\
	main-with-runners;version=snapshot,\
	my.bundle.of.test3;version=snapshot,\
	my.fragment.of.test4;version=snapshot
	
runbundles.tester: \
