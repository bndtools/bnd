Bundle-SymbolicName: ${substring;${propertiesname};0;-${length;.bnd}}
Bundle-Description: The Bnd Gradle plugins.

-includepackage: \
    aQute.bnd.gradle.*

-conditionalpackage: \
    aQute.lib.*,\
    aQute.libg.*

-includeresource: \
    resources
