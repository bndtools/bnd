{%- set templateDir = default(templateDir,'default:') %}
{%- import concat(templateDir,'/blocks.twig') as blocks -%}
{%- import concat(templateDir,'/printer.twig') as printer -%}
{%- if length(report.bundles) == 1 or (length(report.bundles) > 1 and not empty(currentBsn)) -%}
{%- set myBundle = report.bundles[0] -%}
{%- if not empty(currentBsn) -%}
{%- for r in report.bundles -%}
{%- if currentBsn == r.manifest.bundleSymbolicName.symbolicName -%}
{%- set myBundle = r -%}
{%- endif -%}
{%- endfor -%}
{%- endif -%}
{%- do report.putAll(myBundle) -%}
{%- do report.get('bundles').clear() -%}
{%- endif -%}

